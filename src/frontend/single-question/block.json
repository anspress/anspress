{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "anspress/single-question",
  "version": "0.1.0",
  "title": "(AnsPress) Single Question",
  "category": "anspress",
  "icon": "admin-users",
  "description": "AnsPress block to show single question.",
  "attributes": {
    "currentQuestionId": {
      "type": "string",
      "default": ""
    },
    "avatarSize": {
      "type": "number",
      "default": 40
    },
    "commentAvatarSize": {
      "type": "number",
      "default": 30
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./js/index.js",
  "render": "file:./php/single-question.php",
  "style": [
    "file:./js/style-index.css",
    "wp-tinymce"
  ],
  "viewScript": [
    "file:./js/view.js",
    "wp-tinymce"
  ]
}
